{
    "expo": {
      "name": "assettracker",
      "slug": "assettracker",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true,
          "bundleIdentifier": "com.company",
          "config": {
              "googleMapsApiKey": "id"
          }
      },
      "android": {
        "package": "com.company",
            "config": {
                "googleMaps": {
                    "apiKey": "id"
                }
              },
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#FFFFFF"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      }
    }
  }